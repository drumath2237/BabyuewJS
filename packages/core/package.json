{
  "name": "@babyuewjs/core",
  "private": false,
  "version": "1.0.0-alpha.0",
  "description": "Vue3 componets for create 3D Scenes using Babylon.js",
  "author": "drumath2237",
  "bugs": {
    "url": "https://github.com/drumath2237/BabyuewJS/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drumath2237/BabyuewJS"
  },
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "compile": "vue-tsc",
    "build": "pnpm compile && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "pnpm format && pnpm lint"
  },
  "dependencies": {
    "@babylonjs/core": "^6.25.0",
    "vue": "^3.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.8.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.18.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.1",
    "vue-tsc": "^1.8.20"
  }
}
